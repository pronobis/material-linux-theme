! XTerm material color scheme

xterm*foreground:      [material:view-fg]
xterm*background:      [material:view-bg]

xterm*color0:          [material:dark-black]
xterm*color1:          [material:dark-red]
xterm*color2:          [material:dark-green]
xterm*color3:          [material:dark-yellow]
xterm*color4:          [material:dark-blue]
xterm*color5:          [material:dark-magenta]
xterm*color6:          [material:dark-cyan]
xterm*color7:          [material:dark-white]
xterm*color8:          [material:black]
xterm*color9:          [material:red]
xterm*color10:         [material:green]
xterm*color11:         [material:yellow]
xterm*color12:         [material:blue]
xterm*color13:         [material:magenta]
xterm*color14:         [material:cyan]
xterm*color15:         [material:white]

xterm*highlightColor:  [material:view-selected-bg]
xterm*cursorColor:     [material:cursor-normal-bg]
